<%= form_with(model: order, local: true, class: 'margin-bottom-md') do |form| %>
  <div class="field form-group">
    <%= form.label :user_id %>
    <%= form.number_field :user_id, id: :order_user_id, class: 'form-control'%>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
